{
    "compilerOptions": {
      "target": "es6",                 
      "module": "commonjs",
      "lib": ["es6"],
      "outDir": "out",                 // ビルド後の出力先ディレクトリ
      "rootDir": "src",                // TypeScript ソースのルート
      "strict": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "exclude": [
      "node_modules",
      ".vscode-test"
    ]
  }
  